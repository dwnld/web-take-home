{
  "private": true,
  "dependencies": {
    "angular": "^1.5.8",
    "express": "4.14.0",
    "immutability-helper": "^2.0.0",
    "lodash": "^4.17.2",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babelify": "7.3.0",
    "concurrently": "3.0.0",
    "nodemon": "1.10.2",
    "watchify": "3.7.0"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "scripts": {
    "watch:js": "watchify lib/init.js -o assets/app.js -t [ babelify ] -d",
    "watch:server": "nodemon -e js -i assets --exec 'node server.js'",
    "start": "concurrently 'npm run watch:js' 'npm run watch:server'"
  }
}
